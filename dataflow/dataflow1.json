{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "MoviesADLS"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          year as string,",
				"          imdb as string,",
				"          title as string,",
				"          test as string,",
				"          clean_test as string,",
				"          binary as string,",
				"          budget as string,",
				"          domgross as string,",
				"          intgross as string,",
				"          code as string,",
				"          {budget_2013$} as string,",
				"          {domgross_2013$} as string,",
				"          {intgross_2013$} as string,",
				"          {period code} as string,",
				"          {decade code} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesADLS",
				"MoviesADLS select(mapColumn(",
				"          year,",
				"          imdb,",
				"          title,",
				"          test,",
				"          clean_test2 = clean_test,",
				"          binary,",
				"          budget,",
				"          domgross,",
				"          intgross,",
				"          code,",
				"          {budget_2013$},",
				"          {domgross_2013$},",
				"          {intgross_2013$},",
				"          {period code},",
				"          {decade code}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}