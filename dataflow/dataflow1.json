{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "MoviesADLS"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          year as string,",
				"          imdb as string,",
				"          title as string,",
				"          test as string,",
				"          clean_test as string,",
				"          binary as string,",
				"          budget as string,",
				"          domgross as string,",
				"          intgross as string,",
				"          code as string,",
				"          {budget_2013$} as string,",
				"          {domgross_2013$} as string,",
				"          {intgross_2013$} as string,",
				"          {period code} as string,",
				"          {decade code} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesADLS"
			]
		}
	}
}